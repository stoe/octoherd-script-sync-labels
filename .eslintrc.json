{
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "plugins": ["github"],
  "extends": ["plugin:github/recommended"],
  "rules": {
    "camelcase": [
      "error",
      {
        "allow": ["full_name", "html_url", "new_name", "per_page"]
      }
    ]
  }
}
